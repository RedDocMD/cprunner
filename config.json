{
    "c++": {
        "ext": ["cpp"],
        "commands": ["g++ -o ${filenameWithoutExt} ${filename}", "${filenameWithoutExt}"]
    },
    "python": {
        "ext": ["py"],
        "commands": ["python3 ${filename}"]
    },
    "java": {
        "ext": ["java"],
        "commands": ["javac ${filename}", "java ${filenameWithoutExt}"]
    },
    "golang": {
        "ext": ["go"],
        "commands": ["go run ${filename}"]
    }
}